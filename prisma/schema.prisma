generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Investimentos{
  id Int @id @default(autoincrement())
  nome String @db.VarChar(45)
  tipo String @db.VarChar(45)
  valor Boolean
  data DateTime
}